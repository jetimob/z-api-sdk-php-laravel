{
    "name": "jetimob/z-api-sdk-php-laravel",
    "description": "Laravel SDK for Z-API",
    "keywords": ["sdk", "laravel", "php", "z-api"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jetimob\\ZApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetimob\\ZApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ricardo Rohrs",
            "email": "rerohrs@inf.ufsm.br"
        },
        {
            "name": "Jetimob",
            "email": "contato@jetimob.com",
            "homepage": "https://jetimob.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jetimob\\ZApi\\ZApiServiceProvider"
            ],
            "aliases": {
                "ZApi": "Jetimob\\ZApi\\Facades\\ZApi"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "jetimob/http-php-laravel": "^2",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/container": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.20"
    },
    "support": {
        "issues": "https://github.com/jetimob/z-api-sdk-php-laravel/issues"
    }
}
